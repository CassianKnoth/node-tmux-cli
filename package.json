{
	"name": "node-tmux",
	"version": "1.0.0",
	"description": "",
	"main": "index.js",
	"type": "module",
	"scripts": {
		"schema:generate": "rm -rf schema.json && typescript-json-schema tsconfig.json NodeTmuxConfig --topRef --aliasRefs --noExtraProps --required > ./src/config/schema/schema.json",
		"schema:validate": "node .github/scripts/validate-schema.js",
		"prebuild": "npm run schema:generate",
		"build": "rm -rf dist && tsc && cp -r ./src/config dist/",
		"start": "node dist/index.js"
	},
	"keywords": [],
	"author": "",
	"license": "MIT",
	"devDependencies": {
		"@types/node": "^25.0.8",
		"typescript": "^5.9.3",
		"typescript-json-schema": "^0.67.1"
	},
	"dependencies": {
		"ajv": "^8.17.1"
	}
}
